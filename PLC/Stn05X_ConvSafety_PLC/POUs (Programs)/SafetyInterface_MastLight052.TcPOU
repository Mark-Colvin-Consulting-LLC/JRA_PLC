<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.6">
  <POU Name="SafetyInterface_MastLight052" Id="{e3a7ef95-00c9-420f-acc6-e1f70052f6e9}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM SafetyInterface_MastLight052
VAR

	{attribute 'TcLinkTo' := '.oRawArray[0] := TIIB[N16S00 (AL1332)]^N16S08 (DV2530)^RxPDO^output byte 0;
		.oRawArray[1] := TIIB[N16S00 (AL1332)]^N16S08 (DV2530)^RxPDO^output byte 1;
		.oRawArray[2] := TIIB[N16S00 (AL1332)]^N16S08 (DV2530)^RxPDO^output byte 2;
		.oRawArray[3] := TIIB[N16S00 (AL1332)]^N16S08 (DV2530)^RxPDO^output byte 3;
		.oRawArray[4] := TIIB[N16S00 (AL1332)]^N16S08 (DV2530)^RxPDO^output byte 4;
		.oRawArray[5] := TIIB[N16S00 (AL1332)]^N16S08 (DV2530)^RxPDO^output byte 5;
		.oRawArray[6] := TIIB[N16S00 (AL1332)]^N16S08 (DV2530)^RxPDO^output byte 6;
		.oRawArray[7] := TIIB[N16S00 (AL1332)]^N16S08 (DV2530)^RxPDO^output byte 7'}
	fbMastLight: FB_MastLight;	
	
	oStackLightRed AT %Q*: BOOL;		//Real Output via I/O link mast light
	oStackLightGreen AT %Q*: BOOL;		//Real Output via I/O link mast light
	oStackLightAmber AT %Q*: BOOL;		//Real Output via I/O link mast light
	oStackLightHorn AT %Q*: BOOL;		//Real Output via I/O link mast light

	b500msPulse: BOOL;
	tmrPulseON: TON;
	tmrPulseOFF: TON;

END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[(*Call output code*)
Output();

(*1/2 second pulse generator*)
tmrPulseON(IN:=NOT tmrPulseOFF.Q,PT:=T#0.5s);
tmrPulseOFF(IN:=tmrPulseON.Q,PT:=T#0.5s);
IF tmrPulseON.Q THEN
	b500msPulse := TRUE;
ELSE
	b500msPulse := FALSE;
END_IF]]></ST>
    </Implementation>
    <Action Name="Output" Id="{50567a98-6683-47e8-9c0c-e394008b646a}">
      <Implementation>
        <ST><![CDATA[(******Outputs******)

IF (b500msPulse AND
		(gConsumed_Stn051ToSafetyInterface.Enc052MastLight.RedFlashReq OR
		gConsumed_Stn052ToSafetyInterface.Enc052MastLight.RedFlashReq OR
		gConsumed_Stn053ToSafetyInterface.Enc052MastLight.RedFlashReq OR
		gConsumed_Stn054ToSafetyInterface.Enc052MastLight.RedFlashReq OR
		gConsumed_Stn055ToSafetyInterface.Enc052MastLight.RedFlashReq)) OR
		(gConsumed_Stn051ToSafetyInterface.Enc052MastLight.RedReq OR
		gConsumed_Stn052ToSafetyInterface.Enc052MastLight.RedReq OR
		gConsumed_Stn053ToSafetyInterface.Enc052MastLight.RedReq OR
		gConsumed_Stn054ToSafetyInterface.Enc052MastLight.RedReq OR
		gConsumed_Stn055ToSafetyInterface.Enc052MastLight.RedReq) THEN
	oStackLightRed := TRUE;
ELSE
	oStackLightRed := FALSE;
END_IF

IF (b500msPulse AND //Pulsed light
		(gConsumed_Stn051ToSafetyInterface.Enc052MastLight.GrnFlashReq OR
		gConsumed_Stn052ToSafetyInterface.Enc052MastLight.GrnFlashReq OR
		gConsumed_Stn053ToSafetyInterface.Enc052MastLight.GrnFlashReq OR
		gConsumed_Stn054ToSafetyInterface.Enc052MastLight.GrnFlashReq OR
		gConsumed_Stn055ToSafetyInterface.Enc052MastLight.GrnFlashReq)) OR
		((gConsumed_Stn051ToSafetyInterface.Enc052MastLight.GrnReq OR 
		gConsumed_Stn052ToSafetyInterface.Enc052MastLight.GrnReq OR
		gConsumed_Stn053ToSafetyInterface.Enc052MastLight.GrnReq OR
		gConsumed_Stn054ToSafetyInterface.Enc052MastLight.GrnReq OR
		gConsumed_Stn055ToSafetyInterface.Enc052MastLight.GrnReq) AND
		NOT (gConsumed_Stn051ToSafetyInterface.Enc052MastLight.GrnFlashReq OR //Only stay solid green if not flashing
		gConsumed_Stn052ToSafetyInterface.Enc052MastLight.GrnFlashReq OR
		gConsumed_Stn053ToSafetyInterface.Enc052MastLight.GrnFlashReq OR
		gConsumed_Stn054ToSafetyInterface.Enc052MastLight.GrnFlashReq OR
		gConsumed_Stn055ToSafetyInterface.Enc052MastLight.GrnFlashReq)) THEN
	oStackLightGreen := TRUE;
ELSE
	oStackLightGreen := FALSE;
END_IF

IF (b500msPulse AND
		(gConsumed_Stn051ToSafetyInterface.Enc052MastLight.YelFlashReq OR
		gConsumed_Stn052ToSafetyInterface.Enc052MastLight.YelFlashReq OR
		gConsumed_Stn053ToSafetyInterface.Enc052MastLight.YelFlashReq OR
		gConsumed_Stn054ToSafetyInterface.Enc052MastLight.YelFlashReq OR
		gConsumed_Stn055ToSafetyInterface.Enc052MastLight.YelFlashReq)) OR
		(gConsumed_Stn051ToSafetyInterface.Enc052MastLight.YelReq OR
		gConsumed_Stn052ToSafetyInterface.Enc052MastLight.YelReq OR
		gConsumed_Stn053ToSafetyInterface.Enc052MastLight.YelReq OR
		gConsumed_Stn054ToSafetyInterface.Enc052MastLight.YelReq OR
		gConsumed_Stn055ToSafetyInterface.Enc052MastLight.YelReq) THEN
	oStackLightAmber := TRUE;
ELSE
	oStackLightAmber := FALSE;
END_IF

// This logic is commented out because the other station still performs the oscillation
//(*IF (Local.State = 0 AND b500msPulse) OR (pStn^.CycleFaulted AND Local.State <> 0) OR hiTestIndicatorTree THEN
//	oFaultAck_ResetPBLight := TRUE;
//ELSE
//	oFaultAck_ResetPBLight := FALSE;
//END_IF*)

//(* Add horn beep pattern logic*)
//(* Are the conditions RIGHT FOR honking? *)
//(* Supress horn if not acknowledged after suitable period of time *)
//tmrSupressHorn(IN:=(Local.State = 0 OR Local.State = 10),PT:=T#60S);

//DoBeep :=	(Local.State = 0 AND NOT tmrSupressHorn.Q) OR
//			(Local.State = 10 AND NOT tmrSupressHorn.Q AND NOT SeparateAckAndReset) OR
//			Local.State = 150 OR
//			hiTestIndicatorTree;

//tmrBeepPatternON(IN:=NOT tmrBeepPatternOFF.Q,PT:=T#1.0S);
//tmrBeepPatternOFF(IN:=DoBeep AND tmrBeepPatternON.Q,PT:=T#1.0s);


IF (gConsumed_Stn051ToSafetyInterface.Enc052MastLight.HornReq OR
		gConsumed_Stn052ToSafetyInterface.Enc052MastLight.HornReq OR
		gConsumed_Stn053ToSafetyInterface.Enc052MastLight.HornReq OR
		gConsumed_Stn054ToSafetyInterface.Enc052MastLight.HornReq OR
		gConsumed_Stn055ToSafetyInterface.Enc052MastLight.HornReq) THEN
	oStackLightHorn := TRUE;
ELSE
	oStackLightHorn := FALSE;
END_IF

fbMastLight(
	GreenLight := oStackLightGreen,
	YellowLight := oStackLightAmber,
	RedLight := oStackLightRed,
	Horn := oStackLightHorn);
]]></ST>
      </Implementation>
    </Action>
